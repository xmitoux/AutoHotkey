; Alt + Enter => (VSCode) Shift + Ctrl + Enter
!Enter:: Send "{Home}{Enter}{Up}"

; Ctrl + Shift + v => (VSCode) Paste
^+v:: Send "{Home}{Enter}{Up}^v"

; Alt + n => PageDown
!n:: Send "{PgDn}"

; Alt + p => PageUp
!p:: Send "{PgUp}"

; Alt + [ => Ctrl + PageUp
![:: Send "^{PgUp}"

; Alt + ] => Ctrl + PageDown
!]:: Send "^{PgDn}"